{
  "name": "skeleton-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint . --cache --cache-location 'cache/.eslintcache'",
    "lint:prettier": "prettier --check .",
    "lint:style": "stylelint 'src/**/*.{css,scss,sass}' --cache --cache-location 'cache/.stylelintcache'",
    "fix": "run-s fix:eslint fix:prettier fix:style",
    "fix:eslint": "eslint . --fix --cache",
    "fix:prettier": "prettier --write .",
    "fix:style": "stylelint --fix 'src/**/*.{css,scss,sass}' --cache",
    "clean:eslint": "rimraf cache/.eslintcache",
    "clean:style": "rimraf cache/.stylelintcache",
    "clean:lint": "run-p -l -c clean:eslint clean:style",
    "clean:icon": "rimraf cache/.iconcache",
    "clean:opt": "rimraf cache/.imgoptcache",
    "clean:gen": "rimraf cache/.imggencache",
    "clean:resize": "rimraf cache/.imgresizecache",
		"color": "run-p -l -c --aggregate-output color:*",
		"color:palette": "node npm-scripts/palette_gen.js",
		"color:altcolor": "node npm-scripts/altcolor_gen.js",
    "image": "run-s image:resize image:opt image:gen image:icon",
    "image:opt": "node npm-scripts/opt.js",
    "image:gen": "node npm-scripts/gen.js",
    "image:resize": "node npm-scripts/resize.js",
    "image:icon": "node npm-scripts/icon.js"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.15.0",
    "@material/material-color-utilities": "^0.3.0",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/builder-vite": "^8.4.5",
    "@storybook/html": "^8.4.5",
    "@storybook/html-vite": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "browserslist": "^4.24.2",
    "css-declaration-sorter": "^7.2.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fs": "^0.0.1-security",
    "imagemin": "^9.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "npm-run-all2": "^7.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "sass-embedded": "^1.81.0",
    "storybook": "^8.4.5",
    "stylelint": "^16.10.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "terser": "^5.36.0",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@storybook/manager-api": "^8.4.5",
    "@storybook/preview-api": "^8.4.5",
    "sharp": "^0.33.5"
  },
  "browserslist": [
    "last 1 versions",
    "> 1%"
  ]
}
